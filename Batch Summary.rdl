    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select top 100 b.BatchID, a.BatchTypeDesc as [Batch Type], b.ACHBatchTypeID, cast(b.DateEff as DATE) as [Effective Date], ed.TotalCnt as [Total Count], f.TotDebitAmt as [Batch Amount] --,SUBSTRING(cast(b.DateEff as varchar(6)),1,2), b.* --*
into #temp
from Accounting.dbo.BatchHeader b with(nolock)
inner join Accounting.dbo.FileTrailer f with(nolock)
	on b.FileID = f.FileID
inner join Accounting.dbo.ACHBatchTypes a with(nolock)
	on a.ACHBatchTypeID = b.ACHBatchTypeID
cross APPLY
(
	select count(*) as TotalCnt
	from Accounting.dbo.EntryDetail e with(nolock)
	where e.BatchID = b.BatchID
) ed
where a.SystemType = 'Amtrust'
	--and b.DateDescriptive = case when LEN( Cast(DATEPART(DAY, getdate()) as varchar(10))) = 1
	--							then substring(datename(month,getdate()),0,4) + ' 0' + Cast(DATEPART(DAY, getdate()) as varchar(10))
	--							else substring(datename(month,getdate()),0,4) + ' ' + Cast(DATEPART(DAY, getdate()) as varchar(10)) end
and b.DateDescriptive = substring(datename(month,getdate()),0,4) + ' ' + CASE 
																		WHEN len(Cast(DATEPART(DAY, getdate()) as varchar(10))) = 1 THEN '0' + Cast(DATEPART(DAY, getdate()) as varchar(10)) 
																		ELSE Cast(DATEPART(DAY, getdate()) as varchar(10)) 
																		END 
and substring(cast(DATEPART(YEAR,getdate()) as varchar(4)),3,6) = SUBSTRING(cast(b.DateEff as varchar(6)),1,2)
order by b.BatchID 

select *
from #temp</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
