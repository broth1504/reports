select *
into #Final
	from (
	select t.RecordType, t.BillType, t.PolicyID, t.Policy, t.LOB, t.DateEffective, t.DateExpiration, t.DateCanc, t.DateAudit, t.EFT, t.BillDaily,
	t.PreparedDate as ReinstatementDate,
	t.TotalBilled,
	t.TotalPaid, 
	t.PrePostPaid,
	t.HasInvoiceToday,
	t.CurrentDueDate,
	t.CurrentInvDate,
	t.LastInvDate,
	t.CurrentBalance,
	t.MasterAccountID,
	t.BillDailyDayOfMonth,
	t.LastDayPolicyShouldInvoice,
	t.CollectionID,
	t.LastInvoice,
	t.MACBalance
	from #Instl t


	union all


	select t.RecordType, t.BillType, t.PolicyID, t.Policy, t.LOB, t.DateEffective, t.DateExpiration, t.DateCanc, t.DateAudit, t.EFT, t.BillDaily,
	t.PreparedDate as ReinstatementDate,
	t.TotalBilled,
	t.TotalPaid, 
	0 as PrePostPaid,
	case
		when t.CurrentInvDate = @ReportDate then 1
		else 0
	end as HasInvoiceToday,
	t.CurrentDueDate,
	t.CurrentInvDate,
	NULL as LastInvDate,
	t.CurrentBalance,
	t.MasterAccountID,
	t.BillDailyDayOfMonth,
	t.CurrentInvDate as LastDayPolicyShouldInvoice,
	t.CollectionID,
	t.LastInvoice,
	t.CurrentBalance as MACBalance
	from #Down t


	union all


	select t.RecordType, t.BillType, t.PolicyID, t.Policy, t.LOB, t.DateEffective, t.DateExpiration, t.DateCanc, t.DateAudit, t.EFT, t.BillDaily,
	t.PreparedDate as ReinstatementDate,
	t.TotalBilled,
	t.TotalPaid, 
	t.PrePostPaid,
	t.HasInvoiceToday,
	t.CurrentDueDate,
	t.CurrentInvDate,
	t.LastInvDate,
	t.CurrentBalance,
	t.MasterAccountID,
	t.BillDailyDayOfMonth,
	t.LastDayPolicyShouldInvoice,
	t.CollectionID,
	t.LastInvoice,
	t.CurrentBalance as MACBalance
	from #Reinst t
	) f


	select *
	from #Final f
	order by f.Policy


		
