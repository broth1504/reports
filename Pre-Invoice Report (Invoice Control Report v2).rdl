-------------------------------------Start Down Payments Queries----------------------------------
--declare @ReportDate datetime 
--set @ReportDate = '11/20/2020'
--DECLARE @PreviousDay datetime = DATEADD(dd, CASE WHEN DATEPART(dw,@ReportDate) = 2 THEN -3 ELSE -1 END, @ReportDate)

--JKS 5/20/2020 - added a placeholder for MortgageeID since it's going to be needed for CPP Down Pays
Select DISTINCT 'DownPayment' as RecordType, m.MasterAccountId, WC.*, NULL AS MortgageeID, p.TotalBilled, 0 as TotalPaid, p.TotalBilled as CurrentBalance, pc.PolicyCost,
case
	when datediff(d, @ReportDate, wc.DateEffective) &gt; 15 then wc.DateEffective
	else DATEADD(d, 15, @ReportDate)
end as CurrentDueDate, m.BillDailyDayOfMonth, l.CurrentInvDate
INTO #WCDown

FROM #WC WC
cross apply (select top 1 mad.*, ma.AgtID, ma.BillDailyDayOfMonth
				from GrandCentral.dbo.MasterAccountDetail mad with(nolock)
				inner join GrandCentral.dbo.MasterAccount ma with(nolock)
					on mad.MasterAccountId = ma.MasterAccountId
				where wc.PolicyID = mad.PolicyID
				--JKS 5/20/2020 - added the LOB filter when joining to MasterAccountDetail, otherwise it returns a lot of extra records
				and 1 = case
							when wc.LOB = 'WC'
								and mad.LOB like 'WC%'
								and mad.LOB &lt;&gt; 'WCO' then 1
							when wc.LOB = mad.LOB then 1
							else 0
						end
				and mad.DeletedDate is null 
				order by mad.MasterAccountId) m
cross apply (select sum(pia.PremInvAmt) as TotalBilled
				from #WCPremInvAmts pia 
				where wc.PolicyID = pia.PolicyID
				AND 1 = case
							when pia.PremInvDate &lt;= @ReportDate then 1
							when pia.Downpayment = 1 then 1
							else 0
						end) p
cross apply (select min(pia.PremInvDate) as CurrentInvDate
				from #WCPremInvAmts pia 
				where wc.PolicyID = pia.PolicyID
				AND 1 = case
							when pia.PremInvDate &lt;= @ReportDate then 1
							when pia.Downpayment = 1 then 1
							else 0
						end) l
cross apply (select sum(pia.PremInvAmt) as PolicyCost
				from #WCPremInvAmts pia 
				where wc.PolicyID = pia.PolicyID) pc					 
--Don't need to add an outer apply to #WCAcctRec because we're already excluding policies with even a single record below
WHERE wc.BillType in ('A', 'I')
--and wc.policyid = 3229893
--Down pays limit to just these WC LOB variations
and wc.LOB in('WC', 'WCB', 'WCP')
and WC.DateEffective &gt; DATEADD(YY, -1, @ReportDate)
--and isnull(WC.EFT, 0) = 0
and 1 = case
			when m.DateInvoiced is null then 1
			when cast(M.DateInvoiced as date) &gt;= @ReportDate then 1
			when cast(m.DateInvoiced as date) &lt; @ReportDate then 0 
		end
and M.DeletedDate IS NULL
and m.AgtID &lt;&gt; 47
and ((SELECT COUNT(*) FROM #WCAcctRec AR WHERE AR.PolicyID = WC.PolicyID)
				+ (SELECT COUNT(*) FROM Accounting.dbo.AcctRecPrePost AR WITH(NOLOCK) WHERE AR.LOB LIKE 'WC%' AND AR.PolicyID = WC.PolicyID AND cast(ar.EnteredDateTime as date) &lt; @ReportDate)) = 0
and (SELECT COUNT(*)
       FROM Accounting.dbo.InvoiceDetails ID WITH(NOLOCK)
	    INNER JOIN Accounting.dbo.InvoiceMaster IM WITH(NOLOCK)
		 ON IM.InvoiceNumber = ID.InvoiceNumber
		 AND IM.InvoiceType &lt;&gt; 30
       WHERE ID.LOB LIKE 'WC%'
        AND ID.PolicyID = WC.PolicyID
        AND ID.BalanceDue &gt; 0
		AND im.InvoiceDate &lt;&gt; @ReportDate
		AND im.InvoiceDate &lt; @ReportDate) = 0
--JKS 5/26/2020 - extra validations needed to match original 
and wc.pmtplan not in(2,4)
--JKS 10/14/2020 - per Marcy, only invoice individual policies with an amount billed over $50
and p.TotalBilled &gt;= 50


--declare @ReportDate datetime 
--set @ReportDate = '10/23/2020'
Select DISTINCT 'DownPayment' as RecordType, ma.MasterAccountId, CPP.*, MG.CPPPolicyAdditionalInsured_ID AS MortgageeID, 
p.TotalBilled, isnull(a.TotalPaid, 0) as TotalPaid, p.TotalBilled - isnull(a.TotalPaid, 0) as CurrentBalance, pc.PolicyCost,
case
	when datediff(d, @ReportDate, cpp.DateEffective) &gt; 15 then cpp.DateEffective
	else DATEADD(d, 15, @ReportDate)
end as CurrentDueDate, ma.BillDailyDayOfMonth, l.CurrentInvDate
INTO #CPPDown

--select distinct md.MasterAccountId, md.PolicyId, md.LOB
FROM #CPP CPP
inner join GrandCentral.dbo.MasterAccountDetail md with(nolock)
	on cpp.PolicyID = md.PolicyID
	--JKS 5/20/2020 - added the LOB filter when joining to MasterAccountDetail, otherwise it returns a lot of extra records
	and cpp.LOB = md.LOB 
	and md.DeletedDate is null
inner join GrandCentral.dbo.MasterAccount ma with(nolock)
	on md.MasterAccountId = ma.MasterAccountId
LEFT JOIN CPP.dbo.CPPPolicyAdditionalInsured MG WITH(NOLOCK) 
	ON CPP.PmtPlan = 51
	AND MG.CPPPolicyAdditionalInsured_ID = (SELECT TOP 1 S.CPPPolicyAdditionalInsured_ID
											FROM CPP.dbo.CPPPolicyAdditionalInsured S WITH(NOLOCK)
											WHERE S.PolicyID = CPP.PolicyID
											AND S.AdditionalInsuredType = 'Mortgagee Bill'
											ORDER BY S.AdditionalID)
cross apply (select sum(pia.PremInvAmt) as TotalBilled
				from #CPPPremInvAmts pia 
				where CPP.PolicyID = pia.PolicyID
				AND 1 = case
							when pia.PremInvDate &lt;= @ReportDate then 1
							when pia.Downpayment = 1 then 1
							else 0
						end) p
cross apply (select sum(pia.PremInvAmt) as PolicyCost
				from #CPPPremInvAmts pia 
				where CPP.PolicyID = pia.PolicyID) pc
cross apply (select min(pia.PremInvDate) as CurrentInvDate
				from #CPPPremInvAmts pia 
				where cpp.PolicyID = pia.PolicyID
				AND 1 = case
							when pia.PremInvDate &lt;= @ReportDate then 1
							when pia.Downpayment = 1 then 1
							else 0
						end) l					
outer apply (select sum(ar.PaidPrem) as TotalPaid
				from #CPPAcctRec ar
				where cpp.PolicyID = ar.PolicyID) a
outer apply (select sum(arp.PaidPrem) as PrePaid
				from Accounting.dbo.AcctRecPrePost arp WITH(nolock)
				where cpp.PolicyId = arp.PolicyID
				and arp.LOB = 'CPP'
				and arp.EnteredDateTime &lt; @ReportDate) pp
WHERE cpp.BillType in ('A', 'I')
--and cpp.policyid = 1537838
and CPP.DateEffective &gt; DATEADD(YY, -1, GETDATE())
and 1 = case
			when md.DateInvoiced is null then 1
			when cast(MD.DateInvoiced as date) &gt;= @ReportDate then 1
			when cast(md.DateInvoiced as date) &lt; @ReportDate then 0 
		end
and MD.DeletedDate IS NULL
--Brought forward the ProductType limiter 
and CPP.ProductType &lt;&gt; 'SPPASE'
--and CPP.EFT = 0
and (SELECT COUNT(*)
       FROM Accounting.dbo.InvoiceDetails ID WITH(NOLOCK)
	    INNER JOIN Accounting.dbo.InvoiceMaster IM WITH(NOLOCK)
		 ON IM.InvoiceNumber = ID.InvoiceNumber
		 AND IM.InvoiceType &lt;&gt; 30
       WHERE ID.LOB ='CPP'
        AND ID.PolicyID = CPP.PolicyID
        AND ID.BalanceDue &gt; 0
		AND im.InvoiceDate &lt;&gt; @ReportDate
		AND im.InvoiceDate &lt; @ReportDate
		) = 0
--JKS 5/26/2020 - extra validations needed to match original 
and 1 = case
			when cpp.BillType = 'A' then 1
			when cpp.BillType = 'I' 
				--JKS 10/14/2020 - per Marcy, only invoice individual policies with an amount billed over $50
				and p.TotalBilled - isnull(a.TotalPaid, 0) - isnull(pp.PrePaid, 0) &gt;= 50 then 1
			else 0
		end
and ma.AgtID &lt;&gt; 47


select c.*, cc.CollectionID, li.LastInvoice
into #Down
from (select *
		from #WCDown 

		union all

		select *
		from #CPPDown
		) c
left join collections.dbo.CollCollections cc (nolock)
	on c.PolicyID = cc.PolicyID
	and 1 = case	
				when c.LOB like 'WC%' 
					and cc.LOB like 'WC%' then 1
				when c.LOB = cc.LOB then 1
				else 0
			end
	and cc.Outstanding = 1

outer apply (select max(im.InvoiceDate) as LastInvoice
				from dbo.InvoiceMaster im (nolock)
				inner join dbo.InvoiceDetails id (nolock)
					on im.InvoiceNumber = id.InvoiceNumber
				where id.PolicyID = c.PolicyID
				and 1 = case	
							when c.LOB like 'WC%' 
								and id.LOB like 'WC%' then 1
							when c.LOB = id.LOB then 1
							else 0
						end
				and id.BalanceDue &gt; 0	
				and im.AmountDue &gt;= 50	
						) li

