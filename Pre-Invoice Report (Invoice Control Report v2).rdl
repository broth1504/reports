        <CommandText>use Accounting

declare @ReportDate datetime = cast(GetDate()+1 as date) 
DECLARE @PreviousDay datetime = DATEADD(dd, CASE WHEN DATEPART(dw,GETDATE()) = 2 THEN -3 ELSE -1 END, @ReportDate)


select top 1 *,
dateadd(d, -1, cast(cast(DATEPART(m, DATEADD(m, 1, @ReportDate)) as varchar(2)) + '/1/' +  cast(DATEPART(YEAR, DATEADD(m, 1, @ReportDate)) as varchar(4)) as date)) as LastDayOfCurrentMonth,
dateadd(d, -1, cast(cast(DATEPART(m, @ReportDate) as varchar(2)) + '/1/' +  cast(DATEPART(YEAR, @ReportDate) as varchar(4)) as date)) as LastDayOfLastMonth
into #LastClose
from GrandCentral.dbo._ApprovedEOMDates aed (nolock)
where aed.EOMProcessing &lt;= @ReportDate
order by aed.EOMDate desc


--GET THE LIST OF ALL POLICIES THAT HAVE BEEN INVOICED IN THE LAST 30 DAYS, 
--IF THEY'VE RECEIVED AN INVOICE LATELY, THEY SHOULDN'T GET ANOTHER
SELECT distinct id.PolicyID, id.LOB
into #InvoicedPolicies
FROM Accounting.dbo.InvoiceMaster im (NOLOCK)
INNER JOIN Accounting.dbo.InvoiceDetails id (NOLOCK)
		ON id.InvoiceNumber = im.InvoiceNumber
WHERE im.InvoiceType &lt;&gt; 30
AND im.AmountDue &gt;= 50
AND id.BalanceDue &gt; 0
and id.LOB &lt;&gt; 'MAC'
--changed to use &gt; vs &gt;= for months where there are only 30 days
AND im.InvoiceDate &gt; DATEADD(DAY, -30, DATEDIFF(DAY, 0, @ReportDate))
and im.PrintedDate &lt; dateadd(HOUR, -1, @ReportDate)





--FIND ELIGIBLE WC POLICIES, BALANCE CHECK IS LATER
--declare @ReportDate datetime = '11/20/2020'
select pd.PolicyID, pd.Policy, 'WC' as LOB, pd.DateEffective, pd.DateExpiration, pd.DateCanc, pd.DateAudit, pd.Eft, pd.BillDaily, reinst.PreparedDate, pd.BillType, 
--JKS 5/20/2020 - had to add PmtPlan and ProductType to filter Down Payments appropriately
'WC' as ProductType, pd.PmtPlan, pd.AgtGroup, pd.NumPmts, pd.PriorPolicy, pd.UwOfc
into #WC
from premdat.dbo.PolicyData pd (nolock)
outer apply (select top 1 eh.PreparedDate
				from Premdat.dbo.EndmtHistory eh (nolock)
				where pd.PolicyID = eh.PolicyID
				and eh.RateType like '%Reinst%'
				and eh.PreparedDate &lt; @ReportDate
				order by eh.IndexID desc) reinst
where 1 = 1 
--and pd.policyid in(2719335, 2865435, 3115081, 2942463)
and pd.AssignedRisk = 0
and pd.AgtId &lt;&gt; 47
and (pd.DateCanc is null
	or pd.DateAudit is not null)
and pd.AgtBoundDate &lt; @ReportDate
and 1 = case
			--any policy that has been audited and Stop Audit Invoice is not turned on should be included
			when pd.DateAudit is not null 
				and isnull(pd.StopAuditInvoice, 0) = 0 then 1
			--non-audited PAYO, MSA, PSR policies should be excluded
			--JKS 5/20/2020 - removed the PmtPlan check because to use #WC for Down Payments, only PAYO and PSR are excluded
			--when pd.PmtPlan in(2,3,4) then 0
			--non-audited policies with Stop Invoice should be excluded
			when pd.DateAudit is null
				and isnull(pd.StopInvoice, 0) = 1 then 0
			--policies with Stop Audit Invoice should be excluded
			when isnull(pd.StopAuditInvoice, 0) = 1 then 0
			--non-cancelled, non-audited, no Stops in place, should be included
			when pd.PmtPlan in(2,3,4) then 0
			else 1
		end

--HAD TO MOVE THE 30 DAY EXCLUSION TO THE BALANCE CHECK BECAUSE THAT'S ONLY APPLICABLE TO POLICIES THAT ARE PAST THEIR LAST INVOICE DATE
--FOR MONTHS FOLLOWING A 30 DAY MONTH (LIKE NOVEMBER) ALL THE NORMAL POLICIES THAT SHOULD PICK UP WERE GETTING EXCLUDED BECAUSE THE
--LAST INVOICE WAS EXACTLY 30 DAYS PRIOR
----if the policy has been included on an invoice that was eligible to be printed in the last 30 days it should be excluded
--AND NOT EXISTS(select 1
--				from #InvoicedPolicies i
--				where pd.PolicyID = i.PolicyID
--				and i.LOB like 'WC%')



--FIND ELIGIBLE CPP POLICIES, BALANCE CHECK IS LATER
--declare @ReportDate datetime = '11/20/2020'
select pd.PolicyID, pd.Policy + '-' + pd.PolicySuffix as Policy, 'CPP' as LOB, pd.DateEffective, pd.DateExpiration, pd.DateCanc, pd.AuditDate as DateAudit, cp.EFT, cp.BillDaily, reinst.DateProcessed, pp.BillType,
--JKS 5/20/2020 - had to add PmtPlan and ProductType to filter Down Payments appropriately
pd.ProductType, pd.PmtPlan, pd.AgtGroup, pp.NumPayments, pd.PriorPolicy, ag.UwOfc
into #CPP
from CPP.dbo.CPPPolicySubData_View pd (nolock)
inner join CPP.dbo.CPPPolicyCore cp (nolock)
	on pd.PolicyID = cp.PolicyID
inner join Premdat.dbo.AgtGroup ag (nolock)
	on pd.AgtGroup = ag.AgtGroup 
inner join CPP.dbo.PaymentPlans pp (nolock)
	on pd.PmtPlan = pp.PPID
outer apply (select top 1 eh.DateProcessed
				from CPP.dbo.CPPPolicyEndmtHistory eh (nolock)
				where pd.PolicyID = eh.PolicyID
				and eh.TransactionCode = 'REI'
				and eh.DateProcessed &lt; @ReportDate
				order by eh.SeqID desc) reinst
cross apply (select eh.DateProcessed
				from CPP.dbo.CPPPolicyEndmtHistory eh (nolock)
				where pd.PolicyID = eh.PolicyID
				and eh.EndmtNum = 0) bound

where 1 = 1 
--and pd.Policyid	= 1551788
and pd.AgtId &lt;&gt; 47
and (pd.DateCanc is null
	or pd.AuditDate is not null)
and bound.DateProcessed &lt; @ReportDate
and 1 = case
			when pd.AuditDate is not null 
				and isnull(cp.DoNotInvoiceAfterAudit, 0) = 0 then 1
			when pd.AuditDate is null
				and isnull(cp.DoNotInvoice, 0) = 1 then 0
			when isnull(cp.DoNotInvoiceAfterAudit, 0) = 1 then 0
			else 1
		end
--HAD TO MOVE THE 30 DAY EXCLUSION TO THE BALANCE CHECK BECAUSE THAT'S ONLY APPLICABLE TO POLICIES THAT ARE PAST THEIR LAST INVOICE DATE
--FOR MONTHS FOLLOWING A 30 DAY MONTH (LIKE NOVEMBER) ALL THE NORMAL POLICIES THAT SHOULD PICK UP WERE GETTING EXCLUDED BECAUSE THE
--LAST INVOICE WAS EXACTLY 30 DAYS PRIOR
----if the policy has been included on an invoice that was eligible to be printed in the last 30 days it should be excluded
