--declare @ReportDate datetime 
--set @ReportDate = '10/23/2020'
--DECLARE @PreviousDay datetime = DATEADD(dd, CASE WHEN DATEPART(dw,@ReportDate) = 2 THEN -3 ELSE -1 END, @ReportDate)

Select 'Reinst' as RecordType, mad.MasterAccountId,
WCBal.*, pia.PremInvDate as LastDayPolicyShouldInvoice, ma.BillDailyDayOfMonth
INTO #WCReinst
FROM #WCBal WCBal
inner join GrandCentral.dbo.MasterAccountDetail mad WITH(nolock)
	on wcbal.PolicyID = mad.PolicyId
	and mad.LOB like 'WC%'
	and mad.LOB &lt;&gt; 'WCO'
	and mad.DeletedDate is null
inner join GrandCentral.dbo.MasterAccount ma (nolock)
	on mad.MasterAccountID = ma.MasterAccountID
cross apply (select sum(pia.PremInvAmt) as TotalBilled
				from #WCPremInvAmts pia WITH(nolock)
				where WCBal.Policyid = pia.PolicyID
				and pia.PremInvDate &lt;= @ReportDate) p

outer apply (select sum(ar.PaidPrem) as TotalPaid
				from #WCAcctRec ar WITH(nolock)
				where WCBal.PolicyID = ar.PolicyID) a

outer apply (select sum(arp.PaidPrem) as PrePostPaid
				from Accounting.dbo.AcctRecPrePost arp WITH(nolock)
				where WCBal.PolicyID = arp.PolicyID
				and arp.LOB like 'WC%'
				and arp.EnteredDateTime &lt; @ReportDate) pre
CROSS APPLY (SELECT TOP 1 *
				FROM Premdat.dbo.PremInvAmts OA WITH(NOLOCK)
				WHERE OA.PolicyID = WCBal.PolicyID
				 AND OA.PremInvDate &lt;= @ReportDate
				 AND OA.PremDueDate IS NOT NULL
				 AND OA.PremDueDate &gt;= DATEADD(dd, 12, @ReportDate)
				 AND ISNULL(OA.Reinst, 0) = 0
				 AND OA.PremInvDesc NOT LIKE '%Reinst%'
				 AND OA.PremInvDesc NOT LIKE '%Fee%'
				ORDER BY PremInvDate DESC
			  ) PIA
WHERE WCBal.PreparedDate BETWEEN @PreviousDay and @ReportDate
AND NOT EXISTS(SELECT TOP 1 1
				  FROM Accounting.dbo.InvoiceDetails ID WITH(NOLOCK)
				  LEFT JOIN Accounting.dbo.InvoiceMaster IM WITH(NOLOCK) 
					ON IM.InvoiceNumber = ID.InvoiceNumber
				  WHERE ID.LOB like 'WC%'
				  AND ID.PolicyID = WCBal.PolicyID
				  AND ID.BalanceDue &gt; 0
				  AND IM.InvoiceDate = PIA.PremInvDate
				  AND (IM.InvoiceDate = @ReportDate
					OR IM.PrintedDate &gt;= IM.InvoiceDate))
and p.TotalBilled - isnull(a.TotalPaid, 0) - isnull(pre.PrePostPaid, 0) &gt; 0 
and WCBal.BillType = 'I'



--declare @ReportDate datetime 
--set @ReportDate = '10/23/2020'
--DECLARE @PreviousDay datetime = DATEADD(dd, CASE WHEN DATEPART(dw,@ReportDate) = 2 THEN -3 ELSE -1 END, @ReportDate)

Select 'Reinst' as RecordType, mad.MasterAccountId,
CPPBal.*, pia.PremInvDate as LastDayPolicyShouldInvoice, ma.BillDailyDayOfMonth
INTO #CPPReinst
FROM #CPPBal CPPBal
inner join GrandCentral.dbo.MasterAccountDetail mad WITH(nolock)
	on CPPbal.PolicyID = mad.PolicyId
	and mad.LOB = 'CPP'
	and mad.DeletedDate is null
inner join GrandCentral.dbo.MasterAccount ma (nolock)
	on mad.MasterAccountID = ma.MasterAccountID
cross apply (select sum(pia.PremInvAmt) as TotalBilled
				from #CPPPremInvAmts pia WITH(nolock)
				where CPPBal.Policyid = pia.PolicyID
				and pia.PremInvDate &lt;= @ReportDate) p

outer apply (select sum(ar.PaidPrem) as TotalPaid
				from #CPPAcctRec ar WITH(nolock)
				where CPPBal.PolicyID = ar.PolicyID) a

outer apply (select sum(arp.PaidPrem) as PrePostPaid
				from Accounting.dbo.AcctRecPrePost arp WITH(nolock)
				where CPPBal.PolicyID = arp.PolicyID
				and arp.LOB = 'CPP'
				and arp.EnteredDateTime &lt; @ReportDate) pre
CROSS APPLY (SELECT TOP 1 oa.PremInvDate
				FROM CPP.dbo.PremInvAmts OA WITH(NOLOCK)
				WHERE OA.PolicyID = CPPBal.PolicyID
				 AND OA.PremInvDate &lt;= @ReportDate
				 AND OA.PremDueDate IS NOT NULL
				 AND OA.PremDueDate &gt;= DATEADD(dd, 12, @ReportDate)
				 AND ISNULL(OA.Reinst, 0) = 0
				 AND OA.PremInvDesc NOT LIKE '%Reinst%'
				 AND OA.PremInvDesc NOT LIKE '%Fee%'
				ORDER BY PremInvDate DESC
			  ) PIA
where CPPBal.DateProcessed BETWEEN @PreviousDay and @ReportDate
AND NOT EXISTS(SELECT TOP 1 1
				  FROM Accounting.dbo.InvoiceDetails ID WITH(NOLOCK)
				  LEFT JOIN Accounting.dbo.InvoiceMaster IM WITH(NOLOCK) 
					ON IM.InvoiceNumber = ID.InvoiceNumber
				  WHERE ID.LOB = 'CPP'
				  AND ID.PolicyID = CPPBal.PolicyID
				  AND ID.BalanceDue &gt; 0
				  AND IM.InvoiceDate = PIA.PremInvDate
				  AND (IM.InvoiceDate = @ReportDate
					OR IM.PrintedDate &gt;= IM.InvoiceDate))
and p.TotalBilled - isnull(a.TotalPaid, 0) - isnull(pre.PrePostPaid, 0) &gt; 0
and CPPBal.BillType = 'I'


select c.*, cc.CollectionID, li.LastInvoice
into #Reinst
from (select *
		from #WCReinst 

		union all

		select *
		from #CPPReinst) c
left join collections.dbo.CollCollections cc (nolock)
	on c.PolicyID = cc.PolicyID
	and 1 = case	
				when c.LOB like 'WC%' 
					and cc.LOB like 'WC%' then 1
				when c.LOB = cc.LOB then 1
				else 0
			end
	and cc.Outstanding = 1

outer apply (select max(im.InvoiceDate) as LastInvoice
				from dbo.InvoiceMaster im (nolock)
				inner join dbo.InvoiceDetails id (nolock)
					on im.InvoiceNumber = id.InvoiceNumber
				where id.PolicyID = c.PolicyID
				and 1 = case	
							when c.LOB like 'WC%' 
								and id.LOB like 'WC%' then 1
							when c.LOB = id.LOB then 1
							else 0
						end
				and id.BalanceDue &gt; 0	
				and im.AmountDue &gt;= 50	
						) li
