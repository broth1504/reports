--declare @ReportDate datetime = '10/23/2020'
--was getting false positives when calculating the MAC balance prior to filtering out policies in Collections
select *
into #Instl
from #Filtered c
cross apply (select sum(mc.CurrentBalance) as MACBalance
				from #Filtered mc
				where c.MasterAccountID = mc.masterAccountID) bal
where bal.MACBalance &gt;= 50
and NOT EXISTS(Select distinct 1
				from #WCDown W
				where c.PolicyID = w.PolicyID
				and c.LOB like 'WC%'

				union all

				Select distinct 1
				from #WCReinst  W
				where c.PolicyID = w.PolicyID
				and c.LOB like 'WC%'
		
				union all

				Select distinct 1 
				from #CPPDown W
				where c.PolicyID = w.PolicyID
				and c.LOB = 'CPP'

				union all
		
				Select distinct 1 
				from #CPPReinst W
				where c.PolicyID = w.PolicyID
				and c.LOB = 'CPP')
--JKS 10/14/2020 - add exclusion for any non-Direct Bill for AUI office codes
and 1 = CASE
			WHEN c.BillType &lt;&gt; 'I'
				and c.UwOfc in(10,20,24,26,36) then 0
			ELSE 1
		END


