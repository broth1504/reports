  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>IF OBJECT_ID('tempdb..#TeamsTemp') IS NOT NULL drop table #TeamsTemp
use Developers_Views	
SELECT B.areapath, 
       B.teamname
INTO #TeamsTemp
FROM   (SELECT *, 
               Row_number() 
                 OVER ( 
                   partition BY A.areapath 
                   ORDER BY A.teampriority, A.teamname) RN 
        FROM   (SELECT DISTINCT V.teamfieldvalue AreaPath, 
                                T .teamname, 
                                CASE 
                                  WHEN T .teamname LIKE '%(D)%' THEN 0 
                                  ELSE 
                                    CASE 
                                      WHEN T .teamname LIKE '%(S)%' THEN 1 
                                      ELSE 2 
                                    END 
                                END              TeamPriority 
                FROM   dbo._ref_rest_vststeams T 
                       JOIN dbo._ref_rest_vststeamfieldvalues V 
                         ON T .teamid = V.teamid 
                WHERE  V.fieldreferencename = 'System.AreaPath'
				) A
			) B 

WHERE  
	rn = 1	
--------------------------------------------------------------------------------

Select
	isnull(wr.PortfolioPriority,9999) as PortfolioPriority,
	wr.WorkRequestID,
	wr.Title,
	WR.WorkRequestStateValue,
	WI.WorkItemID as 'Feature ID',
	WI.Title as 'Feature Title',
	isnull(WI.IterationLevel3,0) as 'Sprint',
	isnull(WI.AreaLevel3 + ' - ' + WI.AreaLevel4,WI.AreaLevel3) as Application,
	WI.State as 'Feature State',
	WR.PortfolioValue + ' - ' + wr.SponsoringBusinessUnitBillTo as PortfolioandBU,
	WR.BUOrder,
	WR.ID,
	WI.TeamName
	
from 
	Developers_Views.._ref_MPOProjectData_WorkRequests Wr with (nolock)
left join
    (
        SELECT
            VS.WorkItemID, VS.Title, VS.IterationLevel3, VS.AreaLevel3, VS.AreaLevel4, VS.State, tt.Teamname, VS.Workrequestnumber    
        FROM developers_Views.._ref_VSTSData VS with (nolock)
        join #teamstemp tt
        on VS.areapath = tt.areapath
            and tt.teamname like '%External%'
        WHERE
            VS.State &lt;&gt; 'Removed'
    ) 
	WI
		on WR.WorkRequestID = WI.Workrequestnumber  

WHERE
	Wr.ImpactedBUSubBU like '%ets.External%' and
	Wr.WorkRequestStateValue not in ('Draft (default)','Closed','Canceled')
	and (WI.teamname like '%external%' or WI.teamname is null)

Order BY
	isnull(PortfolioPriority,9999),
	wr.WorkRequestID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
