    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>Select
	distinct
	top 15	
	isnull(wr.PortfolioPriority,9999) as PortfolioPriority,
	wr.WorkRequestID,
	wr.Title,
	WR.WorkRequestStateValue,
	WR.BUOrder,
	WR.PortfolioValue,	
	wr.SponsoringBusinessUnitBillTo as 'SponsoringBU',
	WR.ID

from Developers_Views.._ref_MPOProjectData_WorkRequests Wr with (nolock)


WHERE
	Wr.ImpactedBUSubBU like '%ets.external%' and
	Wr.WorkRequestStateValue not in ('Draft (default)','Closed','Canceled')
	
Order BY
	isnull(PortfolioPriority,9999),
	WR.BUOrder,
	wr.WorkRequestID,
	WR.PortfolioValue,	
	SponsoringBU,
	wr.Title,
	WR.WorkRequestStateValue</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
