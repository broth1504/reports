    <DataSet Name="TotalbugswithouteithertagbyBU">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>Select
	count(WorkItemID) as #OpenBugsWithoutEitherTag,
	AreaLevel2 as BU

from developers_Views.._ref_VSTSData VS with (nolock)

WHERE
	WorkItemType = 'Bug'
	--and VS.AreaPath like '%ETS%'
	and state not in ('Closed','Removed')
	and (Tags not like '%Rework%' or Tags is null)
	and (Tags not like '%Groomed%' or Tags is null)
	and AreaLevel2 not in ('International','Amynta','finance') 
	and AreaLevel2 is not null
	and IterationPath = 'Amtrust'

Group by
	AreaLevel2

Order by #OpenBugsWithoutEitherTag desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
