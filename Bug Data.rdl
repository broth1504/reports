  <DataSets>
    <DataSet Name="Total_bugs_groomed_but_not_closed_or_removed">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>--//****************************************************************************************************************************************************************
-- Total bugs groomed but not closed or removed
Select
	count(WorkItemID) as #ofBugsGroomed

from developers_Views.._ref_VSTSData VS with (nolock)

WHERE
	WorkItemType = 'Bug'
	--and VS.AreaPath like '%ETS%'
	and state not in ('Closed','Removed')
	and Tags like '%Groomed%'
AND (AreaLevel2 NOT LIKE '%Amynta%')</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ID_ofBugsGroomed">
          <DataField>#ofBugsGroomed</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Total_bugs_groomed_but_not_closed_or_removed_by_BU">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        COUNT(WorkItemID) AS #ofBugsGroomed, AreaLevel2 AS BU
FROM            _ref_VSTSData AS VS WITH (nolock)
WHERE        (WorkItemType = 'Bug') AND (State NOT IN ('Closed', 'Removed')) AND (Tags LIKE '%Groomed%') AND (AreaLevel2 NOT LIKE '%Amynta%')
GROUP BY AreaLevel2
ORDER BY #ofBugsGroomed DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="ID_ofBugsGroomed">
          <DataField>#ofBugsGroomed</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="BU">
          <DataField>BU</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Totalbugsgroomedbutnotclosedorremovedbyteam">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>-- Total bugs groomed but not closed or removed by team

IF OBJECT_ID('tempdb..#TeamsTemp') IS NOT NULL drop table #TeamsTemp

SELECT B.areapath, 
       B.teamname
INTO #TeamsTemp
FROM   (SELECT *, 
               Row_number() 
                 OVER ( 
                   partition BY A.areapath 
                   ORDER BY A.teampriority, A.teamname) RN 
        FROM   (SELECT DISTINCT V.teamfieldvalue AreaPath, 
                                T .teamname, 
                                CASE 
                                  WHEN T .teamname LIKE '%(D)%' THEN 0 
                                  ELSE 
                                    CASE 
                                      WHEN T .teamname LIKE '%(S)%' THEN 1 
                                      ELSE 2 
                                    END 
                                END              TeamPriority 
                FROM   dbo._ref_rest_vststeams T 
                       JOIN dbo._ref_rest_vststeamfieldvalues V 
                         ON T .teamid = V.teamid 
                WHERE  V.fieldreferencename = 'System.AreaPath'
				) A
			) B 

WHERE  
	rn = 1	
--------------------------------------------------------------------------------

Select
	count(WorkItemID) as #ofBugsGroomed,
	TT.TeamName

from developers_Views.._ref_VSTSData VS with (nolock)
join #TeamsTemp TT with (nolock)
		on VS.AreaPath = TT.AreaPath

WHERE
	WorkItemType = 'Bug'
	--and VS.AreaPath like '%ETS%'
	and state not in ('Closed','Removed')
	and Tags like '%Groomed%'

group by
	tt.TeamName

Order by #ofBugsGroomed desc</CommandText>
      </Query>
      <Fields>
        <Field Name="ID_ofBugsGroomed">
          <DataField>#ofBugsGroomed</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TeamName">
          <DataField>TeamName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
