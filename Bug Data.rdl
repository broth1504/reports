    <DataSet Name="FullData">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        	
	WorkItemId,
	title as BugTitle,
	AreaLevel2 AS BU,
	AreaLevel3 as Product,
	State,
	IterationPath,
	AreaPath as FullAreaPath,
	Tags,
	'https://dev.azure.com/amtrust/Amtrust/_workitems/edit/' + cast(WorkItemId as varchar(10)) as URL
FROM	
	Developers_Views.._ref_ado_workitemdata
WHERe	
	(WorkItemType = 'Bug') 
	AND (State NOT IN ('Closed', 'Removed')) AND (Tags LIKE '%Groomed%' or Tags like ' %rework%') AND (AreaLevel2 NOT LIKE '%Amynta%')
	
