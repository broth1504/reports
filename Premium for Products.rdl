  <DataSources>
    <DataSource Name="DataSource1">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=amt-reports1;Initial Catalog=CPP</ConnectString>
        <Prompt>Specify a user name and password for data source DataSource1:</Prompt>
      </ConnectionProperties>
      <rd:SecurityType>DataBase</rd:SecurityType>
      <rd:DataSourceID>03d0d8dc-4a8f-423c-95a7-e0134cf0209a</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select 
	PC.ProductType, 
	case when (select Name from premdat.dbo.AgtScoreModel with (nolock) where ID = PC.ScoreModelID) = 'None' then '' else (select Name from premdat.dbo.AgtScoreModel with (nolock) where ID = PC.ScoreModelID) end as 'Score Model', 
	sum(DATA.PremAnnual) as [Premium], 
	COUNT(DATA.PolicyID) as 'Number of Policies'
from cpp..CPPPolicySubData DATA with (nolock)
	inner join cpp..CPPPolicyCore PC
		ON PC.PolicyID = DATA.PolicyID
		and
				DATA.EndmtNum = (
				select top 1 sub2.EndmtNum
				from cpp.dbo.CPPPolicyCore pc2 WITH(NOLOCK)
					inner join cpp.dbo.CPPPolicySubData sub2 WITH(NOLOCK) on sub2.PolicyID = pc.policyid
				where pc2.policyid = pc.policyid
				order by sub2.endmtnum desc
			) 

where 
	AgtID &lt;&gt;47
	and PC.DateExpiration &gt; GETDATE()
	and ScoreModelID &gt; -1

Group By PC.ProductType, PC.ScoreModelID
Order by Premium Desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ProductType">
          <DataField>ProductType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
